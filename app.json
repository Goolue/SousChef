{
  "expo": {
    "name": "SousChef2",
    "slug": "SousChef2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/robo.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/robo.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.goolue.SousChef"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/robo.jpg"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "sdkVersion": "51.0.0",
    "extra": {
      "eas": {
        "projectId": "c81da5d5-cb39-4056-a764-d4b64b8f0b51"
      }
    }
  },
  "name": "souschef2"
}
